{
  "name": "zinfo-logger",
  "version": "1.1.6",
  "description": "Quickly get information about the current directory, system, and node.",
  "keywords": [
    "info",
    "node",
    "nvm"
  ],
  "homepage": "https://github.com/jajaperson/zinfo#readme",
  "bugs": {
    "url": "https://github.com/jajaperson/zinfo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jajaperson/zinfo.git"
  },
  "license": "MIT",
  "author": "James Jensen <jamesarthurjensen@gmail.com>",
  "files": [
    "lib",
    "bin"
  ],
  "main": "lib/zinfo.js",
  "typings": "lib/zinfo.d.ts",
  "directories": {
    "lib": "lib",
    "bin": "bin"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" \"bin/**/*.ts\" \"bin/**/*.js\"",
    "lint": "tslint --project .",
    "prepare": "tsc",
    "prepublishOnly": "npm test && npm run build",
    "release": "np",
    "pretest": "npm run lint",
    "test": "jest",
    "version": "./scripts/update-readme && git add README.md"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "execa": "^1.0.0",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^8.0.1",
    "@types/globby": "^9.1.0",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.136",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "^13.1.8",
    "@types/yargs": "^12.0.12",
    "fs-extra": "^7.0.1",
    "jest": "^24.8.0",
    "np": "^5.0.3",
    "prettier": "^1.18.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
